fof(goal, conjecture,
    ((! [X, Y, Z] :
        multiply(X, multiply(Y, Z)) = multiply(multiply(X, Y), Z) &
      ! [X] : multiply(e, X) = X & ! [X] : multiply(i(X), X) = e) =>
     ! [X] : (multiply(X, X) = X <=> X = e))).
